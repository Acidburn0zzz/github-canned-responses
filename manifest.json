{
  "manifest_version": 2,

  "name" : "Canned Github response",
  "description" : "Choose from a set of canned responses for your Github PRs or issues",
  "version" : "1.0.0",

  "permissions": [
    "activeTab"
  ],

  "options_page": "options.html",

  "content_scripts": [
    {
      "js": [
        "response.js"
      ],
      "matches": ["*://github.com/*"]
    }
  ]
}
